{
    "name": "compredict/ai-sdk",
    "type": "library",
    "description": "Enables PHP applications to communicate with Compredict's AI Core API.",
    "version": "1.0.0-beta",
    "keywords": ["api", "rest", "compredict", "AI", "sdk"],
    "homepage": "http://compredict.de",
    "license": "MIT",
    "authors":[
        {
            "name":"COMPREDICT GmbH",
            "homepage":"https://compredict.de"
        },
        {
            "name":"Ousama Esbel",
            "email": "esbel@compredict.de"
        }
    ],
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.35",
        "php-coveralls/php-coveralls": "2.1",
        "codeless/jugglecode": "1.0",
        "vlucas/phpdotenv": "^2.2"
    },
    "autoload": {
        "psr-0": {
            "Compredict": "src/"
        },
        "psr-4": {
            "Compredict\\Test\\": "test/"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}