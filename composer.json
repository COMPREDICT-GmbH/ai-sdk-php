{
    "name": "compredict/ai-sdk",
    "type": "library",
    "description": "Enables PHP applications to communicate with Compredict's AI Core API.",
    "version": "1.0.3",
    "keywords": ["api", "rest", "compredict", "AI", "sdk"],
    "homepage": "https://compredict.ai",
    "license": "MIT",
    "authors":[
        {
            "name":"COMPREDICT GmbH",
            "homepage":"https://compredict.ai"
        },
        {
            "name":"Ousama Esbel",
            "email": "esbel@compredict.de"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-curl": "*",
        "ext-openssl": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^9.0",
        "vlucas/phpdotenv": "^2.2"
    },
   "autoload": {
        "psr-4": {
             "Compredict\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Compredict\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
